
# # !/bin/bash
# Aliases
alias 'cd..'='cd ..'
alias 'cl'='clear'
alias 'cla'='clear ; ls -la'
alias 'cmds'='head -20 ~/.bashrc'
alias 'gitlog'='git log --oneline --graph --all --decorate'
alias 'gits'='git status -uno'
alias 'cgits'='clear ; git status -uno'
alias 'gitsu'='git status -u no'
alias 'gitbra'='git branch -a'
alias 'gitreba'='git rebase --autostash'
alias 'gitpul'='git pull --all'
alias 'gitfetreba'='git fetch ; git rebase --autostash'
alias hist='history | egrep $1' # Requires one input
alias keyring='eval `sshagent` ; ssh-add'
alias 'la'='ls -la'
alias 'l1'='ls -1'
alias 'lll'='ls -laR'
alias 'gitg'='git graph'

# Settings
export HISTSIZE=50000
export HISTFILESIZE=50000
export HISTTIMEFORMAT="%h %d %H:%M:%S "
export projects=~/projects
export PROMPT_COMMAND='history -a'

